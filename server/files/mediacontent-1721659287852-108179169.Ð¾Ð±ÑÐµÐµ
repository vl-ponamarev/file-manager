8)"4" - 2 = 2
9)"4px" - 2 = NaN
10)"  -9  " + 5 = "  -9  5" // (3) - –∫–æ–Ω–∫–∞—Ç–µ–Ω–∞—Ü–∏—è
11)"  -9  " - 5 = -14 // (4)
12)null + 1 = 1 // (5)
